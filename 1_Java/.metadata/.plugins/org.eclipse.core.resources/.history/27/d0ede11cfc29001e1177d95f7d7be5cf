package edu.kh.oop.practice.model.service;

import edu.kh.oop.practice.model.vo.Hero;

public class HeroService {
	
	public void practice() {
		
		Hero warrior = new Hero();
		Hero wizard = new Hero();
		Hero healer = new Hero();
		
		warrior.setJob("워리어");
		warrior.setHp(18000);
		warrior.setMp(5000);
		warrior.setGender('남');
		warrior.setLevel(5);
		
		wizard.setJob("마법사");
		wizard.setHp(5000);
		wizard.setMp(25000);
		wizard.setGender('여');
		wizard.setLevel(2);
		
		healer.setJob("힐러");
		healer.setHp(10000);
		healer.setMp(50000);
		healer.setGender('남');
		healer.setLevel(10);
	}
}
